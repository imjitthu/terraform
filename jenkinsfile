    agent any
    parameters {
    choice(name: 'action', choices: ['init', 'plan', 'validate', 'apply', 'destroy'], description: 'Pick any action')
               }
    stages {
        stage ('Roboshop App') {
            steps{
                rsWebapp("${params.action}")
            }
        }
    }
}

